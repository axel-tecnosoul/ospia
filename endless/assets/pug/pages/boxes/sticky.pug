- var sticky                = true;
- var jquery_ui             = true;
- var chat_menu             = true;
- var theme_customizer      = true;
- var typeahead             = true;
- var typeahead_search      = true;
- var tooltip               = true;
doctype html
html(lang='en')
    include ../../components/header-files
    body
        include ../../components/loader
        // page-wrapper Start
        .page-wrapper.box-layout
            include ../../components/header
            // Page Body Start
            .page-body-wrapper
                include ../../components/sidebar
                include ../../components/chat-sidebar
                .page-body
                    .container-fluid
                        .page-header
                            .row
                                .col
                                    .page-header-left
                                        h3 Sticky
                                        ol.breadcrumb
                                            li.breadcrumb-item
                                              a(href='index.html')
                                                i(data-feather='home')
                                            li.breadcrumb-item Advance
                                            li.breadcrumb-item.active Sticky
                                include ../../components/bookmark
                    // Container-fluid starts                    
                    .container-fluid
                      .row.sticky-header-main
                        .col-sm-12
                          .card
                            .card-header
                              h5
                                | Sticky Note 
                                a#add_new.btn.btn-primary.pull-right.m-l-10(href='javascript:;') Add New Note
                            .card-body
                              #board.sticky-note

                    // Container-fluid Ends
                include ../../components/footer
        include ../../components/footer-files   
