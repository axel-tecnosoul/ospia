- var chat_menu             = true;
- var theme_customizer      = true;
- var typeahead             = true;
- var typeahead_search      = true;
- var tooltip               = true;
- var googlechart           = true;
- var page_googlechart      = true;

doctype html
html(lang='en')
    include ../../components/header-files
    body
        include ../../components/loader
        // page-wrapper Start
        .page-wrapper
            include ../../components/header
            // Page Body Start
            .page-body-wrapper
                include ../../components/sidebar
                include ../../components/chat-sidebar
                .page-body
                    .container-fluid
                        .page-header
                            .row
                                .col
                                  .page-header-left
                                    h3 Google Chart
                                    ol.breadcrumb
                                        li.breadcrumb-item
                                          a(href='index.html')
                                            i(data-feather='home')
                                        li.breadcrumb-item Charts
                                        li.breadcrumb-item.active Google Chart
                                include ../../components/bookmark
                    // Container-fluid starts
                    .container-fluid
                      .row
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Pie Chart 
                                span.digits 1
                            .card-body.chart-block
                              #pie-chart1.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Pie Chart 
                                span.digits 2
                            .card-body.chart-block
                              #pie-chart2.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Pie Chart 
                                span.digits 3
                            .card-body.chart-block
                              #pie-chart3.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Pie Chart 
                                span.digits 4
                            .card-body.chart-block
                              #pie-chart4.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Area Chart 
                                span.digits 1
                            .card-body.chart-block
                              #area-chart1.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Area Chart 
                                span.digits 2
                            .card-body.chart-block
                              #area-chart2.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Column Chart 
                                span.digits 1
                            .card-body.chart-block
                              #column-chart1.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Column Chart 
                                span.digits 2
                            .card-body.chart-block
                              #column-chart2.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Bars Chart 
                                span.digits 1
                            .card-body.chart-block
                              #bar-chart1.chart-overflow
                        .col-sm-12.col-xl-6
                          .card
                            .card-header
                              h5
                                | Bars Chart 
                                span.digits 2
                            .card-body.chart-block
                              #bar-chart2.chart-overflow
                        .col-sm-12
                          .card
                            .card-header
                              h5 Line Chart
                            .card-body.chart-block
                              #line-chart.chart-overflow
                        .col-sm-12
                          .card
                            .card-header
                              h5 Combo Chart
                            .card-body.chart-block
                              #combo-chart.chart-overflow
                    // Container-fluid Ends
                include ../../components/footer
        include ../../components/footer-files
